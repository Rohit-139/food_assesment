
	<%= form_with model: data, url: home_path do |form| %>

 

  <div align="center">
    <%= form.label :name, "Name" %> <br>
    <%= form.text_field :name, required: true, autofocus: true, autocomplete: "given-name" %>
  </div>
 <br>

  <div align="center">
    <%= form.label :email, "Email Address" %> <br>
    <%= form.email_field :email, required: true, autocomplete: "email" %>
  </div>
<br>
  <div align="center">
    <%= form.label :password %>   <br>
    <%= form.password_field :password, required: true, autocomplete: "new-password" %>
  </div>
<br>
  <div align="center">
    <%= form.label :password_confirmation, "Password Confirmation" %> <br>
    <%= form.password_field :password_confirmation, required: true, autocomplete: "new-password" %>
  </div>
<br>
  <div align="center">
    <%= form.submit "Create Account" %>
  </div>
<% end %>
